{
  "name": "toucan-frontend",
  "description": "Frontend",
  "private": "true",
  "version": "0.0.1",
  "license": "proprietary",
  "scripts": {
    "dev": "sapper dev",
    "style": "postcss src/index.css -o ./static/style.bundled.css",
    "prettier": "prettier src/**/*.{js,svelte} -w",
    "lint": "eslint src/**/*.{js,svelte}",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier && yarn lint"
    }
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.1",
    "cookie-session": "^1.4.0",
    "express": "^4.17.1",
    "express-openid-connect": "^1.0.2",
    "highlight.js": "^10.1.2",
    "node-fetch": "^2.6.0",
    "sirv": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "eslint": "^7.8.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "prettier": "^2.1.1",
    "prettier-plugin-svelte": "^1.2.0",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.0",
    "svelte": "^3.17.3",
    "svelte-preprocess-postcss": "^1.1.1",
    "tailwindcss": "^1.7.6"
  }
}
